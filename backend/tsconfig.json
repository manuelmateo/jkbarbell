{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "strict": true,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true
  }
}
